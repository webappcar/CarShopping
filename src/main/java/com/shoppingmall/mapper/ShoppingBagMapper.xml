<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.shoppingmall.mapper.ShoppingBagMapper">

	<resultMap type="com.shoppingmall.model.ShoppingBag" id="shoppingBagResultMap">
		<id property="id" column="id"/>
		<id property="code" column="code"/>
	</resultMap>
	
	<select id="selectShoppingBag" resultMap="shoppingBagResultMap">
		select * from shopping_bag
	</select>
	
	<delete id="deleteShoppingBag" parameterType="int">
		delete * from shopping_bag where id=#{id}
	</delete>
</mapper>